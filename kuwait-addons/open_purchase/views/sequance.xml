<odoo>
    <data>

        <record id="open_purchase_seq" model="ir.sequence">
            <field name="name">open.purchase</field>
            <field name="code">open.purchase</field>
            <field name="prefix">OP/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
        </record>


        <record id="try_sales_seq" model="ir.sequence">
            <field name="name">try.sales</field>
            <field name="code">try.sales</field>
            <field name="prefix">TRYSALES/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>